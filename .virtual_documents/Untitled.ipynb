def mean_squared_error(m, b, points):
    total_error = 0
    n = len(points)
    for i in range(n):
        y = points.iloc[i].y
        x = points.iloc[i].x

        total_error += (y - (m * x + b)) ** 2

    return (1/n) * total_error



